clear all; close all; clc
fn = 'testFunc';
errx = 0.001;
errf = 0.001;

X = [-10:10];
Y = X;
Z = zeros(length(X), length(Y));

for i = 1:length(X)
    for j = 1:length(Y)
        xstart = [X(i), Y(j)];
        [xmin fmin iter] = Smin(fn,xstart,errx,errf);
        Z(i, j) = iter;
    end
end

surf(X, Y, Z)
xlabel('x')
ylabel('y')
zlabel('Function Evaluations')
